<?xml version="1.0"?>
<layoutml>


<module base="Packages">

	<if OS="!Windows">
		<configuration Feature="Full" />
	</if>

	<if OS="Windows">
		<configuration Feature="${base}" />
	</if>

	<dir name="AddOns" base="${base}">

		<dir name="Packages" src="../../Packages">

			<all exclude=".*,*.nb,Layout,BarAndPieCharts,ClusterAnalysis,ComputationalGeometry2D,Documentation,
					ComputationalGeometry3D,DeveloperUtilities,ErrorBarPlot,ExperimentalFunctions,StatisticsCommon" />

			<dir name="Notation">

				<dir name="LocalPalettes">

					<dir name="English">

						<all/>

					</dir>

				</dir>

			</dir>

		</dir>

	</dir>

</module>


</layoutml>
